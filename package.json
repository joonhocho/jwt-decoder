{
  "name": "jwt-node-decoder",
  "version": "0.0.1",
  "description": "Decodes JWT (JSON Web Token) and checks expiration date. A Node port of angular-jwt.",
  "main": "lib/index.js",
  "scripts": {
    "babel": "BABEL_ENV=babel babel src --out-dir lib",
    "babel-watch": "BABEL_ENV=babel babel src --watch --out-dir lib",
    "start": "npm run babel",
    "test": "mocha testout/**/*.js",
    "test-babel": "BABEL_ENV=babel babel test --out-dir testout",
    "test-babel-watch": "BABEL_ENV=babel babel test --watch --out-dir testout",
    "test-watch": "mocha --watch testout/**/*.js",
    "watch": "npm run babel-watch & npm run test-babel-watch & npm run test-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonhocho/jwt-node-decoder.git"
  },
  "keywords": [
    "jwt",
    "JSON",
    "Web",
    "Token",
    "node",
    "angular-jwt",
    "decoder",
    "expiration",
    "expired"
  ],
  "author": "Joon Ho Cho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joonhocho/jwt-node-decoder/issues"
  },
  "homepage": "https://github.com/joonhocho/jwt-node-decoder#readme",
  "dependencies": {
    "abab": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  }
}
